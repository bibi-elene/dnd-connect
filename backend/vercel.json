{
    "version": 2,
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/docs/(.*)",
            "dest": "/public/$1",
            "methods": ["GET"]
        },
        {
            "src": "/(.*)",
            "dest": "src/main.ts"
        }
    ]
}
